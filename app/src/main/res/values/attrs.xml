<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RangeSeekbar">
        <attr name="absoluteMaxValue" format="integer|float"/>
        <attr name="absoluteMinValue" format="integer|float"/>
        <attr name="singleThumb" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="pile">
        <attr name="interval" format="dimension" />
        <attr name="sizeRatio" format="float" />
        <attr name="scaleStep" format="float" />
        <attr name="displayCount" format="float" />
    </declare-styleable>
</resources>